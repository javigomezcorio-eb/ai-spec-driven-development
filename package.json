{
  "name": "four-screens-menu",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug"
  },
  "dependencies": {
    "@fission-ai/openspec": "^1.1.1",
    "expo": "~54.0.33",
    "react": "latest",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "detox": "^20.47.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0"
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/jest.config.js",
    "apps": {
      "ios.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/FourScreensMenu.app",
        "build": "npx expo run:ios --no-install --device \"iPhone 15\""
      }
    },
    "devices": {
      "ios.simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 15"
        }
      }
    },
    "configurations": {
      "ios.sim.debug": {
        "device": "ios.simulator",
        "app": "ios.debug"
      }
    }
  }
}
