{
  "name": "four-screens-menu",
  "version": "0.1.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "npx expo run:ios --device \"iPhone 17\"",
    "web": "expo start --web",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "test": "jest"
  },
  "dependencies": {
    "expo": "~54.0.33",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "detox": "^20.47.0",
    "jest": "^29.7.0",
    "jest-circus": "^29.7.0",
    "jest-expo": "^54.0.17",
    "react-test-renderer": "^19.2.4"
  },
  "detox": {
    "testRunner": {
      "args": {
        "config": "tests/e2e/jest.config.js"
      }
    },
    "apps": {
      "ios.debug": {
        "type": "ios.app",
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/FourScreensMenu.app",
        "build": "npx expo prebuild -p ios --clean && xcodebuild -workspace ios/FourScreensMenu.xcworkspace -scheme FourScreensMenu -configuration Debug -sdk iphonesimulator -destination 'platform=iOS Simulator,name=iPhone 17,OS=26.2' -derivedDataPath ios/build"
      }
    },
    "devices": {
      "ios.simulator": {
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 17",
          "os": "26.2"
        }
      }
    },
    "configurations": {
      "ios.sim.debug": {
        "device": "ios.simulator",
        "app": "ios.debug"
      }
    }
  }
}
